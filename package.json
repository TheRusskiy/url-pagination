{
  "version": "0.3.6",
  "license": "MIT",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "files": [
    "dist",
    "src"
  ],
  "engines": {
    "node": ">=10"
  },
  "sideEffects": false,
  "scripts": {
    "start": "tsdx watch --tsconfig ./tsconfig.tsdx.json",
    "build": "tsdx build --tsconfig ./tsconfig.tsdx.json && npm run postBuild",
    "postBuild": "ts-node -O '{\"module\":\"commonjs\"}' ./postBuild.ts",
    "test": "tsdx --tsconfig ./tsconfig.tsdx.json test --passWithNoTests",
    "lint": "tsdx lint src test --tsconfig ./tsconfig.tsdx.json",
    "prepare": "tsdx build --tsconfig ./tsconfig.tsdx.json && npm run postBuild",
    "size": "size-limit",
    "analyze": "size-limit --why",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook"
  },
  "peerDependencies": {
    "next": ">=10.0.0",
    "react": ">=16"
  },
  "husky": {
    "hooks": {
      "pre-commit": "tsdx lint"
    }
  },
  "prettier": {
    "printWidth": 80,
    "semi": true,
    "singleQuote": true,
    "trailingComma": "es5"
  },
  "name": "url-pagination",
  "author": "Dmitry Ishkov",
  "module": "dist/index.esm.js",
  "size-limit": [
    {
      "path": "dist/url-pagination.cjs.production.min.js",
      "limit": "11 KB"
    },
    {
      "path": "dist/index.esm.js",
      "limit": "11 KB"
    }
  ],
  "devDependencies": {
    "@babel/core": "^7.15.0",
    "@size-limit/preset-small-lib": "^5.0.3",
    "@storybook/addon-essentials": "^6.3.7",
    "@storybook/addon-info": "^5.3.21",
    "@storybook/addon-links": "^6.3.7",
    "@storybook/addons": "^6.3.7",
    "@storybook/react": "^6.3.7",
    "@types/lodash": "^4.14.172",
    "@types/react": "^17.0.17",
    "@types/react-dom": "^17.0.9",
    "babel-loader": "^8.2.2",
    "classnames": "^2.3.1",
    "eslint-plugin-prettier": "^3.4.0",
    "husky": "^7.0.1",
    "next": "^11.1.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-is": "^17.0.2",
    "size-limit": "^5.0.3",
    "ts-node": "^10.2.0",
    "tsdx": "^0.14.1",
    "tslib": "^2.3.1",
    "typescript": "^4.3.5"
  },
  "dependencies": {
    "lodash": "^4.17.21",
    "lodash-es": "^4.17.21",
    "query-string": "^7.0.1"
  }
}
